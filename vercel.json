{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/web-movil-escolar-webapp",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "angular"
}
